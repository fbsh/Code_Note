   "source": [
    "from pynvml import *\n",
    "nvmlInit()\n",
    "ngpus = nvmlDeviceGetCount()\n",
    "for i in range(ngpus):\n",
    "    handle = nvmlDeviceGetHandleByIndex(i)\n",
    "    gpu_util = nvmlDeviceGetUtilizationRates(handle).gpu\n",
    "    print('GPU %d Utilization = %d%%' % (i, gpu_util))"
   ]
